<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="FAQ Element"
    xtype="panel">
    <items jcr:primaryType="cq:WidgetCollection">
        <address
            jcr:primaryType="cq:Widget"
            fieldLabel="Location"
            name="./address"
            xtype="richtext"/>
        <officeName
            jcr:primaryType="cq:Widget"
            fieldLabel="Office Name"
            name="./officename"
            xtype="textfield"/>
        <longitude
            jcr:primaryType="cq:Widget"
            fieldLabel="Longitude"
            name="./longitude"
            xtype="textfield"/>
        <latitude
            jcr:primaryType="cq:Widget"
            fieldLabel="Latitude"
            name="./latitude"
            xtype="textfield"/>
        <city
            jcr:primaryType="cq:Widget"
            fieldLabel="City"
            name="./city"
            xtype="textfield"/>
        <state
            jcr:primaryType="cq:Widget"
            fieldLabel="State"
            name="./state"
            xtype="textfield"/>
    </items>
</jcr:root>
